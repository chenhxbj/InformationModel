@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ids: <https://w3id.org/idsa/core/> .

# Classes
# -------

ids:Proxy 
    a owl:Class;
    rdfs:label "Proxy" @en;
    rdfs:comment "Proxy used for incoming / outgoing communication of a component" @en;
.

# Properties
# ---------

ids:proxyUsername 
    a owl:DatatypeProperty ;
    rdfs:domain ids:Proxy  ;
    rdfs:range xsd:string ;
    rdfs:label "proxy username"@en ;
    rdfs:comment "Proxy username."@en ;
    .
    
ids:proxyPassword
    a owl:DatatypeProperty ;
    rdfs:domain ids:Proxy ;
    rdfs:range xsd:string ;
    rdfs:label "proxy password"@en ;
    rdfs:comment "Proxy password."@en ;
    .
    
ids:proxyURL
    a owl:DatatypeProperty ;
    rdfs:domain ids:Proxy ;
    rdfs:range xsd:anyURI ;
    rdfs:label "proxy URL"@en ;
    rdfs:comment "Proxy URL"@en ;
    .  